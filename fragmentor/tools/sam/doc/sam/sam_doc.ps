% RANDOM USEFUL FUNCTIONS

% Clear n elements from the stack
/npop {				% x1...xn n npop
  {pop} repeat
} def

% Duplicate the top n elements of the stack as a block
% (Note that this is probably an O(n^2) operation.)
/ndup {				% x1...xn n ndup x1...xn x1...xn
  /ndupcount exch def
  ndupcount { ndupcount -1 roll dup ndupcount 1 add 1 roll } repeat
} def

% A much simpler approach than the one in the book, and easier to use.
/ArrowHead {			% length angle xbase ybase ArrowHead
  gsave
    translate
    rotate
    newpath
    0 moveto	% move to the point
    0 currentlinewidth lineto
    0 currentlinewidth neg lineto
    closepath
    fill
  grestore
} def


% 2-D VECTOR OPERATIONS

/VecDup {			% x y VecDup x y x y
  2 ndup
} def

/VecDot {			% x0 y0 x1 y1 VecDot a
  exch 4 -1 roll mul 3 1 roll mul add
} def

/VecLength {			% x y VecLength
  VecDup VecDot sqrt
} def

/VecScale {			% x y a VecScale ax ay
  dup 4 -1 roll mul 3 1 roll mul
} def

/VecAdd {			% x0 y0 x1 y1 VecAdd x y
  exch 4 -1 roll add 3 1 roll add
} def

/VecSub {			% x0 y0 x1 y1 VecAdd x y
  -1 VecScale VecAdd
} def

/VecAngle {			% x y VecAngle
  exch atan
} def



% FUNCTIONS TO DRAW THE STATE-BOXES FOR A MODULE OF THE CHAIN

% Scratch space for converting numbers to strings.
/numstring 10 string def

% Scale and translate coordinates to draw a node.
/nodecoords {			% x y nodecoords
  translate 0 0 moveto
  nodewidth 2 div dup scale
} def

% Draw a number centered at the origin.
/shownum {			% n shownum
    /Times-Roman findfont 1 scalefont setfont
    numstring cvs dup
    stringwidth pop -2 div -.35 moveto
    show
} def

% Stroke and fill a closed path.
/strokeandfill {
  gsave 0.2 setlinewidth stroke grestore
  gsave 1 setgray fill grestore
} def


% Draw a circle with a number in it.
/NumCircle {			% n x y NumCircle
  gsave
    nodecoords
    newpath 0 0 1 -90 270 arc closepath
    strokeandfill
    shownum
  grestore
} def

% Draw an empty (square) box, leaving the new coordinate system in place
% so that things can be put in the box.
/NBox {				% x y NBox
  nodecoords
  newpath 1 1 moveto -1 1 lineto -1 -1 lineto 1 -1 lineto closepath
  strokeandfill
} def

% Draw a diamond-shaped box.
/DBox {				% x y DBox
    nodecoords
    45 rotate
    newpath 1 1 moveto -1 1 lineto -1 -1 lineto 1 -1 lineto closepath
    -45 rotate
    strokeandfill
} def

% Draw a histogram-sized box.
/HBox {				% x y HistBox
  alphabet PROTEIN eq
  {
    nodecoords
    newpath 1 5 moveto -1 5 lineto -1 -5 lineto 1 -5 lineto closepath
    strokeandfill
  }
  {
    NBox
  }
  ifelse
} def

% Draw a box with a string in it.
/StringBox {			% label x y StringBox
  gsave
    NBox

    /Times-Roman findfont 1 scalefont setfont
    dup
    stringwidth pop dup 2 exch div 1.5 scale -2 div -.35 moveto
    show
  grestore
} def

% Draw a (diamond-shaped) box with a number in it.
/NumBox {			% n x y NumBox
  gsave
    DBox

    dup 0 eq
    { pop }
    {
      /Times-Roman findfont 1 scalefont setfont
      numstring cvs dup
      stringwidth pop -2 div -.35 moveto
      show
    }
    ifelse
  grestore
} def


% Draw an empty (square) box, leaving the new coordinate system in place
% so that things can be put in the box.
/NBox {				% x y NBox
  nodecoords
  newpath 1 1 moveto -1 1 lineto -1 -1 lineto 1 -1 lineto closepath
  strokeandfill
} def


% Draw a line of a histogram with label. Do NOT move up for the next line.
/histline_nomove {			% weight label histline
  % dup 0.5 mul .1 add -.9 exch moveto exch show
  % 0.5 mul .25 add -.45 exch moveto
  21 exch sub dup 0.5 mul -.4 add -.9 exch moveto exch show

  0.5 mul -.25 add -.45 exch moveto
  dup 0 eq { pop } { normfactor mul 1.3 mul 0 rlineto } ifelse
} def

/protein_permute {
%  The proteins can be permuted any way you like by numbering them.
  gsave

    HBox
    %/Times-Roman findfont 0.5 scalefont setfont
    /Courier findfont 0.5 scalefont setfont
    newpath


%  This permutation is: TSAGPEDNQKRHWYFLMIVC
%			12345678901234567890


    alphabet PROTEIN eq
    {					% Draw a PROTEIN (20-valued) histogram.
      0 -5 translate
% Draw a PROTEIN (20-valued) histogram.
      (Y) 14 histline_nomove
      (W) 13 histline_nomove
      (V) 19 histline_nomove
      (T)  1 histline_nomove
      (S)  2 histline_nomove
      (R) 11 histline_nomove
      (Q)  9 histline_nomove
      (P)  5 histline_nomove
      (N)  8 histline_nomove
      (M) 17 histline_nomove
      (L) 16 histline_nomove
      (K) 10 histline_nomove
      (I) 18 histline_nomove
      (H) 12 histline_nomove
      (G)  4 histline_nomove
      (F) 15 histline_nomove
      (E)  6 histline_nomove
      (D)  7 histline_nomove
      (C) 20 histline_nomove
      (A)  3 histline_nomove
    }
    {					% Draw a DNA/RNA (4-valued) histogram.
      0 -1 translate

      alphabet DNA eq { (T) } { (U) } ifelse histline
      (G) histline
      (C) histline
      (A) histline
    }
    ifelse

      0.3 setlinewidth stroke

  grestore
} def


% Draw a line of a histogram with label and move up for the next line.
/histline {			% weight label histline
  -.9 .1 moveto show

  -.45 .25 moveto
  dup 0 eq { pop } { normfactor mul 1.3 mul 0 rlineto } ifelse

  0 .5 translate
} def

% Draw a box with a histogram in it.
/HistBox {			% <4 or 20 emmission wts.> x y HistBox
  gsave

    HBox
    %/Times-Roman findfont 0.5 scalefont setfont
    /Courier findfont 0.5 scalefont setfont
    newpath

    alphabet PROTEIN eq
    {					% Draw a PROTEIN (20-valued) histogram.
      0 -5 translate
      (Y) histline
      (W) histline
      (V) histline
      (T) histline
      (S) histline
      (R) histline
      (Q) histline
      (P) histline
      (N) histline
      (M) histline
      (L) histline
      (K) histline
      (I) histline
      (H) histline
      (G) histline
      (F) histline
      (E) histline
      (D) histline
      (C) histline
      (A) histline

    }
    {					% Draw a DNA/RNA (4-valued) histogram.
      0 -1 translate

      alphabet DNA eq { (T) } { (U) } ifelse histline
      (G) histline
      (C) histline
      (A) histline
    }
    ifelse

    0.3 setlinewidth stroke

  grestore
} def


% Draw the nodes of a Start module.
/StartNodes {			% A G C T inum StartNodes
  0 yinsert NumBox			% INSERT
  (BEGIN) 0 ymatch StringBox		% BEGIN (in place of MATCH)
  alphabet PROTEIN eq { 20 } { 4 } ifelse npop 
					% no DELETE
} def

% Draw the three nodes of a normal module.
/Nodes {			% A G C T inum Nodes
  0 yinsert NumBox					% INSERT
  0 ymatch permute {protein_permute} {HistBox} ifelse	% MATCH
  modnum 0 ydelete NumCircle				% DELETE
} def

% Draw the nodes of an End module. (3 normal nodes + End box)
/EndNodes {			% A G C T inum EndNodes
  Nodes					% INSERT, MATCH, DELETE
  (END) hspace yinsert StringBox	% END
} def


% FUNCTIONS TO DRAW TRANSITIONS LINES AND ARROWS

% Draw an arbitrary transition line (w/o arrowhead).
/TransLine {			% weight x0 y0 x1 y1 TransLine
    newpath moveto lineto

    dup dup normfactor mul 10 mul setlinewidth
    0 eq
    { pop }
    {
      dashthreshold lt { [8] 0 setdash } if
      stroke
    }
    ifelse

    [] 0 setdash
} def

% Draw the D-I transition arrow.
/TransitionDI {			% weight TransitionDI
  gsave
    dup
    newpath
    0 ydelete
    0 yinsert 15 nodewidth .71 mul add add

    TransLine

    0 gt { 10 -90 0 yinsert 15 nodewidth .71 mul add add ArrowHead } if
  grestore
} def

% Draw the M-I transition arrow.
/TransitionMI {			% weight TransitionMI
  gsave
    dup
    newpath
    0 ymatch
    0 yinsert 15 nodewidth .71 mul add sub

    TransLine

    0 gt { 10 90 0 yinsert 15 nodewidth .71 mul add sub ArrowHead } if
  grestore
} def

% DON'T draw the I-I transition arrow, but bury the weight so it will
% become inum after all transitions are drawn.
/TransitionII {			% weight TransitionII
  %pop
  9 1 roll
} def


% Draw the transitions for a Start module (no DELETE state).
/StartTransitions {		% w1 ... w9 StartTransitions inum

  TransitionII				% from this I to this I
  0 yinsert hspace ymatch TransLine	% from this I to next M
  0 yinsert hspace ydelete TransLine	% from this I to next D

  TransitionMI				% from this M to this I
  0 ymatch hspace ymatch TransLine	% from this M to next M
  0 ymatch hspace ydelete TransLine	% from this M to next D

  3 npop

} def

% Draw the transitions for a normal module.
/Transitions {			% w1 ... w9 Transitions inum

  TransitionII				% from this I to this I
  0 yinsert hspace ymatch TransLine	% from this I to next M
  0 yinsert hspace ydelete TransLine	% from this I to next D

  TransitionMI				% from this M to this I
  0 ymatch hspace ymatch TransLine	% from this M to next M
  0 ymatch hspace ydelete TransLine	% from this M to next D

  TransitionDI				% from this D to this I
  0 ydelete hspace ymatch TransLine	% from this D to next M
  0 ydelete hspace ydelete TransLine	% from this D to next D

} def

% Draw the transtions for an End module.
/EndTransitions {		% w1 ... w9 EndTransitions inum

  TransitionII				% from this I to this I
  0 yinsert  hspace yinsert TransLine	% from this I to END
  pop

  TransitionMI				% from this M to this I
  0 ymatch   hspace yinsert TransLine	% from this M to END
  pop

  TransitionDI				% from this D to this I
  0 ydelete  hspace yinsert TransLine	% from this D to END
  pop

} def


% FUNCTIONS TO DRAW COMPLETE MODULES (nodes and transitions)

% Draw a Start module (no DELETE state).
/StartModule {			% mA...mT w1...w9 StartModule
  NewLine
  DrawTitle
  gsave
    hspace colnum mul
    vspace rownum mul
    translate

    StartTransitions
    StartNodes
  grestore
  /colnum colnum 1 add def	% increment column number
  /modnum 1 def			% initialize the module number
} def

% Draw a normal module.
/Module {			% mA...mT w1...w9 Module
  colnum maxcolnum eq { GhostModule NewLine } if

  gsave
    hspace colnum mul
    vspace rownum mul
    translate

    Transitions
    Nodes
  grestore
  /colnum colnum 1 add def	% increment column number
  /modnum modnum 1 add def	% increment the module number
} def

% Draw an End module.
/EndModule {			% mA...mT w1...w9 EndModule

  colnum maxcolnum eq { GhostModule NewLine } if  % Automagic linebreak.

  gsave
    hspace colnum mul
    vspace rownum mul
    translate

    EndTransitions
    EndNodes
  grestore
  % /colnum colnum 2 add def	% increment column number
  NewPage
} def

% Draw a "ghost module" (just the state outlines) for use at the end
% of a line as a preview of the next line.
/GhostModule {			% GhostModule
  gsave
    hspace colnum mul
    vspace rownum mul
    translate

    ghostgraylevel setgray
    modnum 0 ydelete NumCircle
    gsave 0 ymatch HBox grestore
    gsave 0 yinsert DBox grestore

  grestore
  /colnum colnum 1 add def	% increment column number
} def


