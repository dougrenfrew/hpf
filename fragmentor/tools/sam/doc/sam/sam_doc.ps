% HIGH-LEVEL CONTROL FUNCTIONS

% Draw in the title/caption.
/DrawTitle {			% label DrawTitle

  % Move to center of title.
  landscape
  { 11 2 div -8.5 }
  { 8.5 2 div -.11 }
  %{ 8.5 2 div -11 }
  ifelse
  72 scalefactor div VecScale
  moveto

  % Center and draw title.
  /Times-Roman findfont vgap 3 scalefactor mul div scalefont setfont
  title stringwidth pop
  ( - ) stringwidth pop
  %( p. ) stringwidth pop
  pagenum numstring cvs stringwidth pop
  add add
  -2 div vgap 2 div rmoveto
  title show
  ( - ) show
  %( p. ) show
  pagenum numstring cvs show

  % Increment page count:
  /pagenum pagenum 1 add def

} def

% Set up for a fresh page.
/InitPage {

  % Determine sizes and spacings:
  /nodewidth	50	def
  /hgap		70	def
  /vgap		50	def
  /hspace	nodewidth hgap add	def
  alphabet PROTEIN eq
  { /histheight nodewidth 5 mul def }
  { /histheight nodewidth def }
  ifelse
  /vspace	nodewidth 2 mul histheight vgap 4 mul add add neg	def

  % Determine node coordinates:
  /ymatch	histheight 2 div vgap add		def
  /yinsert	histheight dup add nodewidth 2 div add	def
  /yinsert	vgap histheight add vgap add nodewidth 2 div add	def
  /ydelete	vgap nodewidth add yinsert add		def

  % Initialize coordinate system:

  landscape
  {			% for landscape printout
    90 rotate
    /maxcolnum 11   72 mul  hspace scalefactor mul  div truncate 1 sub def
    /maxrownum -8.5 72 mul  vspace scalefactor mul  div truncate def
  }
  {			% for vertical printout
    % 0 11 72 mul translate
    0 10.8 72 mul translate
    /maxcolnum 8.5 72 mul  hspace scalefactor mul  div truncate 1 sub def
    /maxrownum -11 72 mul  vspace scalefactor mul  div truncate def
  }
  ifelse

  scalefactor scalefactor scale

  % Initialize row and column counters:
  /colnum 0 def
  /rownum 0 def

} def


% Start a new line (and new page, if necessary).
/NewLine {
  rownum maxrownum eq
  { NewPage NewLine }
  {
    % Automagic pagebreak.
    /colnum 1 def	  	% return to beginning of line
    /rownum rownum 1 add def	% move down one space
  }
  ifelse
} def

% Start a new page.
/NewPage {
  showpage
  InitPage
} def


